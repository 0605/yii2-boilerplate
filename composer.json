{
  "name": "huijiewei/yii2-boilerplate",
  "type": "project",
  "description": "样板项目",
  "require": {
    "php": "^7.1.3",
    "ext-json": "*",
    "composer-plugin-api": "^2.0",
    "aliyuncs/oss-sdk-php": "^2.3",
    "foxy/foxy": "^1.0",
    "fzaninotto/faker": "^1.9.1",
    "gregwar/captcha": "^1.1",
    "hidehalo/nanoid-php": "^1.1",
    "huijiewei/yii2-foxy": "^2.0",
    "huijiewei/yii2-swagger": "^3.0",
    "huijiewei/yii2-upload": "^3.0",
    "huijiewei/yii2-wechat": "^2.0",
    "symfony/dotenv": "^4.1",
    "yii2tech/spreadsheet": "^1.0",
    "yiisoft/yii2": "^2.0",
    "yiisoft/yii2-swiftmailer": "^2.1"
  },
  "require-dev": {
    "yiisoft/yii2-debug": "^2.1",
    "symfony/var-dumper": "^4.1"
  },
  "provide": {
    "bower-asset/bootstrap": "*",
    "bower-asset/jquery": "*",
    "bower-asset/inputmask": "*",
    "bower-asset/punycode": "*",
    "bower-asset/yii2-pjax": "*",
    "npm-asset/wangeditor": "*",
    "npm-asset/webuploader": "*",
    "bower-asset/typeahead.js": "*",
    "npm-asset/blueimp-file-upload": "*",
    "npm-asset/swagger-ui-dist": "*",
    "npm-asset/cropperjs": "*"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Huijie Wei",
      "email": "huijiewei@outlook.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "app\\core\\": "src/core",
      "app\\modules\\": "src/modules"
    }
  },
  "config": {
    "sort-packages": true,
    "process-timeout": 1800,
    "fxp-asset": {
      "enabled": false
    },
    "foxy": {
      "manager": "npm",
      "manager-options": "--legacy-peer-deps"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://mirrors.aliyun.com/composer/"
    }
  ]
}
