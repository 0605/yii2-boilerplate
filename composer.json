{
  "name": "huijiewei/yii2-boilerplate",
  "type": "project",
  "require": {
    "php": "^7.1.3",
    "yiisoft/yii2": "^2.0",
    "symfony/dotenv": "^4.1",
    "yiisoft/yii2-swiftmailer": "^2.1",
    "huijiewei/yii2-wechat": "^2.0",
    "huijiewei/yii2-swagger": "^2.0",
    "aliyuncs/oss-sdk-php": "^2.3",
    "hidehalo/nanoid-php": "^1.1",
    "fzaninotto/faker": "^1.8"
  },
  "require-dev": {
    "yiisoft/yii2-debug": "^2.0",
    "symfony/var-dumper": "^4.1",
    "phpunit/phpunit": "^7"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Huijie Wei",
      "email": "huijiewei@outlook.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "app\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "tests\\": "tests"
    }
  },
  "config": {
    "process-timeout": 1800,
    "fxp-asset": {
      "enabled": false
    }
  },
  "scripts": {
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    },
    {
      "type": "composer",
      "url": "https://packagist.laravel-china.org"
    }
  ]
}
